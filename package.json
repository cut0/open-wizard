{
  "name": "open-wizard",
  "version": "0.0.2",
  "description": "Converting Open API Schemas",
  "main": "index.js",
  "bin": {
    "open-wizard": "./dist/cli/index.js"
  },
  "scripts": {
    "lint:check": "biome ci",
    "lint:fix": "biome check --fix",
    "typecheck": "tsc --noEmit",
    "build": "tsx ./build.ts",
    "dev": "tsx ./src/cli/main.ts",
    "prd": "pnpm build && node ./dist/cli/index.js",
    "test": "vitest",
    "release": "pnpm build && changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cut0/open-wizard.git"
  },
  "keywords": [
    "OpenAPI"
  ],
  "author": "Cut0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cut0/open-wizard/issues"
  },
  "homepage": "https://github.com/cut0/open-wizard#readme",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.28.0",
    "@types/js-yaml": "4.0.9",
    "@types/node": "22.10.10",
    "esbuild": "0.25.0",
    "typescript": "5.7.3",
    "vitest": "3.0.6"
  },
  "dependencies": {
    "commander": "13.1.0",
    "js-yaml": "4.1.0",
    "openapi-types": "12.1.3",
    "tsx": "4.19.2"
  },
  "packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af"
}

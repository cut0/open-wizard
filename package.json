{
  "name": "open-wizard",
  "version": "0.0.1",
  "description": "Converting Open API Schemas",
  "main": "index.js",
  "bin": {
    "open-wizard": "./dist/cli/index.js"
  },
  "scripts": {
    "lint:check": "biome ci",
    "lint:fix": "biome check --fix",
    "typecheck": "tsc --noEmit",
    "build": "tsx ./build.ts",
    "dev": "tsx ./src/cli/main.ts",
    "prd": "pnpm build && node ./dist/cli/index.js",
    "test": "vitest",
    "release": "pnpm build && changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cut0/open-wizard.git"
  },
  "keywords": [
    "OpenAPI"
  ],
  "author": "Cut0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cut0/open-wizard/issues"
  },
  "homepage": "https://github.com/cut0/open-wizard#readme",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.12",
    "@types/js-yaml": "4.0.9",
    "@types/node": "22.10.10",
    "esbuild": "0.25.0",
    "typescript": "5.7.3",
    "vitest": "3.0.5"
  },
  "dependencies": {
    "commander": "13.1.0",
    "js-yaml": "4.1.0",
    "openapi-types": "12.1.3",
    "tsx": "4.19.2"
  },
  "packageManager": "pnpm@10.3.0+sha512.ee592eda8815a8a293c206bb0917c4bb0ff274c50def7cbc17be05ec641fc2d1b02490ce660061356bd0d126a4d7eb2ec8830e6959fb8a447571c631d5a2442d"
}
